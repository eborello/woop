{
    "manifest_version": 3,
    "name": "Spoil Block",
    "description": "Block spoilers from YouTube.",
    "version": "1.0",
    "key": "",
    "icons": {
        "16": "/images/16_SpoilBlockLogo.png",
        "48": "/images/48_SpoilBlockLogo.png",
        "128": "/images/128_SpoilBlockLogo.png"
    },
    "background": {
        "service_worker": "./background.js"
    },
    "options_page": "./options.html",
    "action": {
        "default_icon": {
            "16": "/images/16_SpoilBlockLogo.png",
            "48": "/images/48_SpoilBlockLogo.png",
            "128": "/images/128_SpoilBlockLogo.png"
        },
        "default_popup": "popup.html"
    },
    "content_scripts": [],
    "permissions": [
        "tabs",
        "activeTab",
        "storage",
        "identity",
        "scripting"
    ],
    "host_permissions": [
        "https://www.youtube.com/*",
        "https://spoilblock.azurewebsites.net/*"
    ]
}